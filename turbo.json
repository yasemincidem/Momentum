{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "cache": false
    }
  }
}